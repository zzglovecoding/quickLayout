{
  "name": "app-name",
  "description": "项目名称",
  "version": "1.0.0",
  "parcel": {
    "publicPath": "/",
    "format": "zip"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=development webpack --config webpack.config.dev.babel.js --color",
    "start": "cross-env NODE_ENV=development webpack serve --config webpack.config.dev.babel.js --color",
    "mock": "mock-server ./mock/data --static ./mock/static --config ./mock/mock.config.js --watch",
    "start-mock": "concurrently \"cross-env npm start\" \"npm run mock\"",
    "lint-js": "eslint -c .eslintrc.prod.js src/ --ext .js,.jsx --fix",
    "lint-css": "stylelint \"src/**/*.{css,less,scss}\" --cache --fix",
    "test": "jest",
    "env": "cross-env node ./script/env.js",
    "package": "cross-env NODE_ENV=production webpack --config webpack.config.prod.babel.js --color"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint -c .eslintrc.prod.js --fix"
    ],
    "src/**/*.{css,less,scss}": [
      "stylelint --cache --fix"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "@babel/runtime": "7.13.10",
    "@easytool/http": "1.1.2",
    "antd": "4.15.0",
    "classnames": "2.3.1",
    "core-js": "3.10.1",
    "echarts": "5.0.2",
    "moment": "2.29.1",
    "path-to-regexp": "1.7.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-router-dom": "5.2.0",
    "regenerator-runtime": "0.13.7"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/eslint-parser": "7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-proposal-pipeline-operator": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.14",
    "@easytool/define-config": "^0.1.0",
    "@easytool/mock-server": "^0.2.2",
    "@easytool/proxy-config": "^0.1.0",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-import-less": "^0.1.6",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.0.1",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.23.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-webpack-plugin": "^2.5.3",
    "file-loader": "^6.2.0",
    "filemanager-webpack-plugin": "^4.0.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.4.1",
    "minimist": "^1.2.5",
    "moment-locales-webpack-plugin": "^1.2.0",
    "postcss": "^8.2.9",
    "postcss-loader": "^5.2.0",
    "postcss-nested": "^5.0.5",
    "postcss-simple-vars": "^6.0.3",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.31.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "devEnvironments": {
    "servers": {
      "local": 5000,
      "mock": 3000
    },
    "proxies": {
      "/api": "http://localhost:3000"
    },
    "globals": {
      "__DEV__": true
    }
  }
}
